  issn = {00489697},
  doi = {10.1016/j.scitotenv.2016.12.122},
  urldate = {2023-12-06},
  abstract = {Here, we present the first detailed analysis of processes by which various current use pesticides (CUPs) and legacy organochlorine pesticides (OCPs) are concentrated in melt ponds that form on Arctic sea ice in the summer, when surface snow is melting and ice eventually breaks up. Four current use pesticides (dacthal, chlorpyrifos, trifluralin, and pentachloronitrobenzene) and one legacy organochlorine pesticide ({$\alpha$}-hexachlorocyclohexane) were detected in ponds in Resolute Passage, Canadian Arctic, in 2012. Melt-pond concentrations changed over time as a function of gas exchange, precipitation, and dilution with melting sea ice. Observed increases in melt-pond concentrations for all detected pesticides were associated with precipitation events. Dacthal reached the highest concentration of all current use pesticides in ponds (95 {$\pm$} 71 pg L-1), a value exceeding measured concentrations in the under-ice (0 m) and 5 m seawater by N 10 and N16 times, respectively. Drainage of dacthal-enriched pond water to the ocean during ice break-up provides an important ice-mediated annual delivery route, adding {\textasciitilde}30\% of inventory in the summer Mixed Layer (ML; 10 m) in the Resolute Passage, and a concentrating mechanism with potential implications for exposures to organisms such as ice algae, and phytoplankton.},
  langid = {english},
  file = {C:\Users\Administrator\Zotero\storage\GM25UHE7\Pućko 等 - 2017 - Current use pesticide and legacy organochlorine pe.pdf}
}

@article{puckoDeliveryOrganicContaminants2015,
  title = {The Delivery of Organic Contaminants to the {{Arctic}} Food Web: {{Why}} Sea Ice Matters},
  shorttitle = {The Delivery of Organic Contaminants to the {{Arctic}} Food Web},
  author = {Pu{\'c}ko, Monika and Stern, Gary A. and Macdonald, Robie W. and Jantunen, Liisa M. and Bidleman, Terry F. and Wong, Fiona and Barber, David G. and Rysgaard, S{\o}ren},
  year = {2015},
