  shorttitle = {Polychlorinated Biphenyls ({{PCBs}}) as Sentinels for the Elucidation of {{Arctic}} Environmental Change Processes},
  author = {Carlsson, Pernilla and Breivik, Knut and {Brorstr{\"o}m-Lund{\'e}n}, Eva and Cousins, Ian and Christensen, Jesper and Grimalt, Joan O. and Halsall, Crispin and Kallenborn, Roland and Abass, Khaled and Lammel, Gerhard and Munthe, John and MacLeod, Matthew and Odland, Jon {\O}yvind and Pawlak, Janet and Rautio, Arja and Reiersen, Lars-Otto and Schlabach, Martin and Stemmler, Irene and Wilson, Simon and W{\"o}hrnschimmel, Henry},
  year = {2018},
