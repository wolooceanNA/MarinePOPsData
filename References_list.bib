  author = {Zhong, Guangcai and Xie, Zhiyong and M{\"o}ller, Axel and Halsall, Crispin and Caba, Armando and Sturm, Renate and Tang, Jianhui and Zhang, Gan and Ebinghaus, Ralf},
  year = {2012},
