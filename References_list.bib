}

@article{bruhnAirSeaGas2003,
  title = {Air/Sea Gas Exchange of {{PCBs}} in the Southern {{Baltic Sea}}},
  author = {Bruhn, Regina and Lakaschus, Soenke and McLachlan, Michael S},
  year = {2003},
  month = aug,
  journal = {Atmospheric Environment},
  volume = {37},
