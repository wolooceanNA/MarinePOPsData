@article{waniaCoZMoPOPFugacitybasedDynamic2006,
  title = {{{CoZMo-POP}} 2 {\textendash} {{A}} Fugacity-Based Dynamic Multi-Compartmental Mass Balance Model of the Fate of Persistent Organic Pollutants},
