@article{berrojalbizAtmosphericOccurrenceTransport2014,
  title = {Atmospheric Occurrence, Transport and Deposition of Polychlorinated Biphenyls and Hexachlorobenzene in the {{Mediterranean}} and {{Black}} Seas},
  author = {Berrojalbiz, N. and {Castro-Jim{\'e}nez}, J. and Mariani, G. and Wollgast, J. and Hanke, G. and Dachs, J.},
  year = {2014},
