  author = {Jantunen, Liisa M. and Wong, Fiona and Gawor, Anya and Kylin, Henrik and Helm, Paul A. and Stern, Gary A. and Strachan, William M. J. and Burniston, Deborah A. and Bidleman, Terry F.},
  year = {2015},
