@article{puckoCurrentUsePesticide2017,
  title = {Current Use Pesticide and Legacy Organochlorine Pesticide Dynamics at the Ocean-Sea Ice-Atmosphere Interface in Resolute Passage, {{Canadian Arctic}}, during Winter-Summer Transition},
  author = {Pu{\'c}ko, Monika and Stern, Gary A. and Burt, Alexis E. and Jantunen, Liisa M. and Bidleman, Terry F. and Macdonald, Robie W. and Barber, David G. and Geilfus, Nicolas-X. and Rysgaard, S{\o}ren},
