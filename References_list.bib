@article{kimDeterminationPartitionCoefficient2018,
  title = {Determination of the Partition Coefficient between Dissolved Organic Carbon and Seawater Using Differential Equilibrium Kinetics},
  author = {Kim, Du Yung and Kwon, Jung-Hwan},
  year = {2018},
  month = sep,
  journal = {Environmental Pollution},
  volume = {240},
  pages = {177--183},
  issn = {02697491},
  doi = {10.1016/j.envpol.2018.04.114},
  urldate = {2023-12-06},
  abstract = {Because the freely dissolved fraction of highly hydrophobic organic chemicals is bioavailable, knowing the partition coefficient between dissolved organic carbon and water (KDOCw) is crucial to estimate the freely dissolved fraction from the total concentration. A kinetic method was developed to obtain KDOCw that required a shorter experimental time than equilibrium methods. The equilibrium partition coefficients of four polychlorinated biphenyls (PCBs) (2,4,40-trichlorobiphenyl (PCB 28), 2,20,3,50-tetrachlorobiphenyl (PCB 44), 2,20,4,5,50-pentachlorobiphenyl (PCB 101), and 2,20,4,40,5,50-hexachlorobiphenyl (PCB 153)) between dissolved organic carbon and seawater (KDOCsw) were determined using seawater samples from the Korean coast. The log KDOCsw values of PCB 28 were measured by equilibrating PCB 28, the least hydrophobic congener, with seawater samples, and the values ranged from 6.60 to 7.20. For the more hydrophobic PCBs (PCB 44, PCB 101, and PCB 153), kinetic experiments were conducted to determine the sorption rate constants (k2) and their log KDOCsw values were obtained by comparing their k2 with that of PCB 28. The calculated log KDOCsw values were 6.57e7.35 for PCB 44, 6.23e7.44 for PCB 101, and 6.35e7.73 for PCB 153. The validity of the proposed method was further confirmed using three less hydrophobic polycyclic aromatic hydrocarbons. This kinetic method shortened the experimental time to obtain the KDOCsw values of the more hydrophobic PCBs, which did not reach phase equilibrium.},
  langid = {english},
  file = {C:\Users\Administrator\Zotero\storage\X5C8LEI7\Kim 和 Kwon - 2018 - Determination of the partition coefficient between.pdf}
}

