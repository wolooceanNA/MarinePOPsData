@article{galban-malagonAtmosphericOccurrenceDeposition2013,
  title = {Atmospheric Occurrence and Deposition of Hexachlorobenzene and Hexachlorocyclohexanes in the {{Southern Ocean}} and {{Antarctic Peninsula}}},
  author = {{Galb{\'a}n-Malag{\'o}n}, Crist{\'o}bal and Cabrerizo, Ana and Caballero, Gemma and Dachs, Jordi},
