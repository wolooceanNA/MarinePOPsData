  title = {Seasonal {{Variation}} of {{Legacy Organochlorine Pesticides}} ({{OCPs}}) {{From East Asia}} to the {{Arctic Ocean}}},
  author = {Zheng, Hongyuan and Gao, Yuan and Xia, Yinyue and Yang, Haizhen and Cai, Minghong},
  year = {2020},
