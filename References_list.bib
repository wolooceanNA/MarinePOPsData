}

@article{galban-malagonDegradativeBiologicalPumps2013,
  title = {The ``{{Degradative}}'' and ``{{Biological}}'' {{Pumps Controls}} on the {{Atmospheric Deposition}} and {{Sequestration}} of {{Hexachlorocyclohexanes}} and {{Hexachlorobenzene}} in the {{North Atlantic}} and {{Arctic Oceans}}},
