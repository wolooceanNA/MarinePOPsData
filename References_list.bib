@article{zhangPesticidesAtmosphereSeawater2022,
  title = {Pesticides in the Atmosphere and Seawater in a Transect Study from the {{Western Pacific}} to the {{Southern Ocean}}: {{The}} Importance of Continental Discharges and Air-Seawater Exchange},
  shorttitle = {Pesticides in the Atmosphere and Seawater in a Transect Study from the {{Western Pacific}} to the {{Southern Ocean}}},
  author = {Zhang, Xue and Zhang, Xianming and Zhang, Zi-Feng and Yang, Pu-Fei and Li, Yi-Fan and Cai, Minghong and Kallenborn, Roland},
  year = {2022},
